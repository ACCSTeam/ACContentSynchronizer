<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:ACContentSynchronizer.ClientGui"
             x:Class="ACContentSynchronizer.ClientGui.App">
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

    <Application.Styles>
        <FluentTheme Mode="Dark"/>
        <StyleInclude Source="avares://Material.Icons.Avalonia/App.xaml"></StyleInclude>
        <Style Selector="Window">
            <Setter Property="FontFamily" Value="resm:Assets.Fonts.Roboto.*.ttf"></Setter>
        </Style>
    </Application.Styles>
</Application>
